  <eSubpackages name="variant_handling" nsURI="http://www.topcased.org/0.11.0/east_adl/variant_handling"
      nsPrefix="variant_handling">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This package contains variant handling entities. Variants can be used to manage different occurences of a common concept or entity making possible to build hierarchies of arbitrary deep.&#xD;&#xA;Actually, only a subset of EAST-ADL entities will be variant handling capable."/>
    </eAnnotations>
    <eAnnotations source="http://www.topcased.org/documentation">
      <details key="documentation" value="These entities will be modeled in the structural view."/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="VariantEntity" abstract="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This is an entity that can have a variant handling capabilities. Actually only a subset of EAST ADL structure can be considered as variant entities."/>
      </eAnnotations>
      <eAnnotations source="http://www.topcased.org/documentation">
        <details key="documentation" value="This entity may have derived properties that are deleted from graphical view for readability purposes."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="nullVariant" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This attribute is used to declare a void realization of a variation point (meaning that it becomes optional)."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="complete" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This attribute is used to declare that the selection criterion of this variant entity is not complete."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="activeVariations" upperBound="-1"
          eType="#//variant_handling/Variation" containment="true" eOpposite="#//variant_handling/Variation/variationPoint">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are all variants of this entity. A variation is used here for a proxy-like representation."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="activeSelections" upperBound="-1"
          eType="#//variant_handling/Selection" containment="true" eOpposite="#//variant_handling/Selection/selector">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are all the variant entitites that are selected by this variant entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="activeExclusions" upperBound="-1"
          eType="#//variant_handling/Exclusion" containment="true" eOpposite="#//variant_handling/Exclusion/excluder">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are all variant entities that cannot exist with this variant entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="passiveVariation" eType="#//variant_handling/Variation"
          eOpposite="#//variant_handling/Variation/variant">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This represents the variation point of this entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="passiveSelections" upperBound="-1"
          eType="#//variant_handling/Selection" eOpposite="#//variant_handling/Selection/selected">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are the selections representing selectors of this variant entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="passiveExclusions" upperBound="-1"
          eType="#//variant_handling/Exclusion" eOpposite="#//variant_handling/Exclusion/excluded">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are the exclusions representing excluders of this variant entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="variationPoint" eType="#//variant_handling/VariantEntity"
          changeable="false" volatile="true" transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show a variation point of this entity (if any).&#xD;&#xA;This property will be shown instead of the variation model object because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="variants" upperBound="-1"
          eType="#//variant_handling/VariantEntity" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show all variants of this entity (if any).&#xD;&#xA;This property will be shown instead of the corresponding variation model objects because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selectors" upperBound="-1"
          eType="#//variant_handling/VariantEntity" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show all selectors of this entity (if any).&#xD;&#xA;This property will be shown instead of the corresponding selection model objects because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selected" upperBound="-1"
          eType="#//variant_handling/VariantEntity" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show all objects selected by this entity (if any).&#xD;&#xA;This property will be shown instead of the corresponding selection model objects because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="excluders" upperBound="-1"
          eType="#//variant_handling/VariantEntity" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show all excluders of this entity (if any).&#xD;&#xA;This property will be shown instead of the corresponding exclusion model objects because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="excluded" upperBound="-1"
          eType="#//variant_handling/VariantEntity" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a derived property used to show all objects excluded by this entity (if any).&#xD;&#xA;This property will be shown instead of the corresponding  exclusion model objects because it is more explicit."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="selectionCriterion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This attribute is the condition required for this object to be selected."/>
        </eAnnotations>
        <eAnnotations source="http://www.topcased.org/documentation">
          <details key="documentation" value="This object is modeled as an attribute instead of a eclass (EAST-ADL specification) because i saw no point in making a complex object for something that represents a condition (for now)."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Variation" eSuperTypes="#//core/EASTADLNamedElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This entity allows adding an arbitrary number of variants for a variant entity. A dedicated object is used instead of a property to provide a better view of the model."/>
      </eAnnotations>
      <eAnnotations source="http://www.topcased.org/documentation">
        <details key="documentation" value="This object will be implemented graphically by a connection."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="variant" eType="#//variant_handling/VariantEntity"
          eOpposite="#//variant_handling/VariantEntity/passiveVariation">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is the actual variant of the variation point of this entity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="variationPoint" lowerBound="1"
          eType="#//variant_handling/VariantEntity" eOpposite="#//variant_handling/VariantEntity/activeVariations">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is the variation point of this variation."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Selection" eSuperTypes="#//core/EASTADLNamedElement">
      <eAnnotations source="http://www.topcased.org/documentation">
        <details key="documentation" value="This object will be implemented graphically by a connection."/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This entity allows user to establish a dependency between variants entities meaning that if one (all)  selector(s) is(are) selected, the dependant entities will also be selected. A Selection model object is used here to allow more explicit modeling."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selector" lowerBound="1"
          eType="#//variant_handling/VariantEntity" eOpposite="#//variant_handling/VariantEntity/activeSelections">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is the selector of this selection."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selected" eType="#//variant_handling/VariantEntity"
          eOpposite="#//variant_handling/VariantEntity/passiveSelections">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are the actual variant entities that are selected (dependant) by the selector of this selection."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="selectionType" eType="#//variant_handling/LogicalOperationKind">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This enumeration specifies whether one or all selectors should be selected for dependant entities to be selected. Possible values are OR and AND."/>
        </eAnnotations>
