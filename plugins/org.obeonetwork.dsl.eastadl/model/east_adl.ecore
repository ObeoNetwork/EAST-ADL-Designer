        <details key="documentation" value="This object will be implemented graphically by a connection."/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This entity allows user to establish a dependency between variant entities that cannot exist together. An exclusion model object is used here to allow more explicit modeling."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="excluder" lowerBound="1"
          eType="#//variant_handling/VariantEntity" eOpposite="#//variant_handling/VariantEntity/activeExclusions">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is the excluder object of this exclusion."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="excluded" eType="#//variant_handling/VariantEntity"
          eOpposite="#//variant_handling/VariantEntity/passiveExclusions">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="These are the actual variant entities that are excluded by the excluder of this exclusion."/>
        </eAnnotations>
      </eStructuralFeatures>
